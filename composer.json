{
    "name": "aleks-new/sms-megafon",
    "description": "Megafon Notifications channel for Laravel 5.3.",
    "keywords": ["laravel", "notifications", "smscentre", "sms", "megafon"],
    "homepage": "https://github.com/aleks-new/SmsMegafon",
    "license": "MIT",
    "authors": [
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com",
            "homepage": "https://github.com/jhaoda"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1 - 5.8",
        "illuminate/notifications": "5.1 - 5.8",
        "illuminate/support": "5.1 - 5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsMegafon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsMegafon\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SmsMegafon\\SmsMegafonServiceProvider"
            ]
        }
    }
}
